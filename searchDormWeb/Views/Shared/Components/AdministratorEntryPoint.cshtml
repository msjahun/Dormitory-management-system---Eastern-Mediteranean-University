@using Microsoft.AspNetCore.Identity
@using Dau.Core.Domain.User;
@using Dau.Services.Users;
@inject SignInManager<User> SignInManager
@inject UserManager<User> UserManager
@inject IUsersService _userService
@if (SignInManager.IsSignedIn(User))
{<secure-content asp-action="Index" asp-controller="Admin" asp-area="">
        <div class="text-center bg-white text-white d-inline-block "  > <strong><a class="" asp-action="Index" asp-controller="Admin">Administration</a></strong> </div>
    </secure-content>
}
