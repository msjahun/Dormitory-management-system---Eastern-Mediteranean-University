<!-- The Modal -->
<div id="myModal" class="modal_filter">




        <div class="card model_filter_content">
            <div class="card-header-sm  bg-primary ">

                <span class="close float-right d-lg-none">&times;</span>
                <h5 class="text-white">@Localizer["Filter"]: </h5>
            </div>

            <div class="card-body">

                <button type="button" class="btn d-block btn-sm btn-outline-primary btn-block btn-icon mb-3 " onclick="reloadPage()">
                    <span class="btn-inner--text">@Localizer["Reset"]</span>

                </button>



                @*<button type="button" class="btn btn-sm btn-primary  px-4"> <span class="btn-inner--text">@Localizer["Search"]</span></button>*@

                <h6>@Localizer["Dormitory type"]</h6>
                <div class="custom-control custom-checkbox mb-3">
                    <input type="checkbox" class="custom-control-input targetFilterInputStatic" id="privateDormitories" checked>
                    <label class="custom-control-label inline" for="privateDormitories">@Localizer["Private dormitories"] @*<span class=" float-right">@option.OptionCount</span>*@</label>

                </div>
                <div class="custom-control custom-checkbox mb-3">
                    <input type="checkbox" class="custom-control-input targetFilterInputStatic" id="schoolDormitories" checked>
                    <label class="custom-control-label inline" for="schoolDormitories">@Localizer["School dormitories"] @*<span class=" float-right">@option.OptionCount</span>*@</label>

                </div>

