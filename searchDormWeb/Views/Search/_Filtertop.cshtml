                <br />

                <h6>@Localizer["Price ({0})", "$ USD"]</h6>

                <div class="range-slider-wrapper ">
                    <!-- Range slider container -->
                    <div id="input-slider-range" class="input-slider targetFilterInputStatic" data-range-value-min="1000" data-range-value-max="7000"></div>
                    <!-- Range slider values -->
                    <div class="row">
                        <div class="col-6">
                            <span class="range-slider-value value-low targetFilterInputStatic" data-range-value-low="800" id="input-slider-range-value-low"></span>
                        </div>
                        <div class="col-6 text-right">
                            <span class="range-slider-value value-high targetFilterInputStatic" data-range-value-high="7000" id="input-slider-range-value-high"></span>
                        </div>
                    </div>
                </div>

                <br />




                <h6>@Localizer["Room area ({0})", "m2"]</h6>

                <div class="input-slider-container mb-4">
                    <div id="input-slider-room" class="input-slider input-slider--red targetFilterInputStatic" data-range-value-min="0" data-range-value-max="100"></div>
                    <!-- Input slider values -->
                    <div class="row mt-3">
                        <div class="col-6">
                            <span id="input-slider-red-value targetFilterInputStatic" class="range-slider-value" data-range-value-low="100"></span>
                        </div>
                    </div>
                </div>

                <br />

                <h6>@Localizer["Availability"]</h6>
                <div class="custom-control custom-checkbox mb-3">
                    <input type="checkbox" class="custom-control-input targetFilterInputStatic" id="ShowAvailable" checked>
                    <label class="custom-control-label inline" for="ShowAvailable">@Localizer["Show only Available"] @*<span class=" float-right">@option.OptionCount</span>*@</label>

                </div>



                <h6>@Localizer["Campains"]</h6>
                <div class="custom-control custom-checkbox mb-3">
