
@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/Shared/_LayoutUserNew.cshtml";
}

<!-- Spotlight -->
<section class="slice slice-lg bg-primary bg-size--cover" id="wallpaper_fullScreen" data-spotlight="fullscreen" style="background-image: url('https://dormitories.emu.edu.tr/PhotoGalleries/dormitories/2016/ugursal/img_1196.jpg?RenditionID=12'); background-position: center 60%; background-repeat: no-repeat; background-size: auto;">
