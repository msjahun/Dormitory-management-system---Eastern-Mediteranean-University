
<div class="box box-success box-solid border_hover">

    <!-- /.box-header -->
    <div class="box-body">

        <div class="row">
            <div class="col-lg-12">
                <!-- /.col -->
                <div class="col-md-12 col-sm-12 col-lg-5">
                    <img class="img-responsive" src="@Url.Content("~/Content/")dist/img/dormitories/IMG_1916.jpg" height="580" alt="Photo">
                </div>

                <div class="col-md-12 col-lg-7 col-sm-12">

                    <h4>
                        <a>
                           @ViewBag.ContentFormat_loading_Dormitory_name
                        </a>
                    </h4>
                    <h4 class="hidden-sm">  <small> @ViewBag.ContentFormat_loading_room_type</small></h4>

                    <h4 class="hidden-sm">   <small> @ViewBag.ContentFormat_loading_gender_allocation</small></h4>

                </div>

            </div>
            <!-- /.col -->
            <div class="col-xs-6 col-sm-12 col-md-6 green_hover">
                <span class="hidden-sm hidden-xs "> <img src="@Url.Content("~/Content/")dist/img/tv.png" alt="Photo"></span> <label>@ViewBag.ContentFormat_sort_by_price : Loading</label>
            </div>

            <div class="col-xs-6 col-sm-12 col-md-6 green_hover">
                <span class="hidden-sm"> <img src="@Url.Content("~/Content/")dist/img/tv.png" class="hidden-sm" alt="Photo"> </span><label>@ViewBag.ContentFormat_sort_by_area : Loading</label>
            </div>

        </div>


    </div>
    <div class="overlay">
        <i class="fa fa-refresh fa-spin"></i>
    </div>
    <!-- /.box-body -->


</div>


