    <!-- page script -->
    <script>
        $(function () {
           
            $('#OnlineUsersTable').on('processing.dt', function (e, settings, processing) {
                if (processing) {
                    Pace.start();
                } else {
                    Pace.stop();
                }
            }).DataTable({
                'paging': true,
                'lengthChange': true,
                "aLengthMenu": [[20, 3, 5], [20, 3, 5]],
                'searching': false,
                'ordering': false,
                'info': true,
                'autoWidth': false,
                "processing": true, // for show progress bar
                "serverSide": true, // for process server side
                "filter": true, // this is for disable filter (search box)
                "orderMulti": true, // for disable multiple column at once
                "ajax": {
                    "url": "@Html.Raw(Url.Action("OnlineUsers", "Users"))",
                    "type": "POST",
                    "data": function (d) {
                        d.myKey = "myValue";
                        d.DiscountName = "myValue";
                        d.CouponCode = "myValue";
                        d.DiscountType = "myValue";
                    },
                    "datatype": "json"
                },
                "columns": [

                   
                    {"data": "userInfo" },
                    {"data": "ipAddress" },
                    {"data": "location" },
                    {"data": "lastActivity" },
                    {"render": function (data, type, full, meta) {
                        return '<a  href="' + full.id + '"><i class="fa fa-pencil"></i> ' + full.id + '</a>'; }
                    }
                    
                    
                ]


            })

        })
    </script>
